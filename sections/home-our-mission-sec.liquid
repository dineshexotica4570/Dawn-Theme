
<section class="our_mission bg_style p_y" style="background-image: url('{{ section.settings.our-mission-imgss | img_url: '1024x1024' }}')">
      <div class="row align-items-center g-0">
        <div class="col-md-6">
          <div class="our_mission_content">
            <span class="cap_txt">{{ section.settings['sub-heading'] }}</span>
            <h2>{{ section.settings['heading'] }}</h2>
            <p>
             {{ section.settings['description'] }}
            </p>
          </div>
        </div>
      </div>
    </section>

{% schema %}
{
  "name": "Home Our Mission section",
  "settings": [
    {
      "type": "image_picker",
      "id": "our-mission-imgss",
      "label": "Our image"
    },
    {
      "type": "text",
      "id": "heading",
      "label": "Our heading"
    },
    {
      "type": "text",
      "id": "sub-heading",
      "label": "Our Sub heading"
    },
    {
      "type": "text",
      "id": "description",
      "label": "Our Description"
    }
  ],
  "presets": [
    {
      "name": "Home Our Mission section",
      "category": "Home Our Mission section"
    }
  ]
}
{% endschema %}